{
  "name": "FireEmblemDrafter",
  "version": "1.0.0",
  "description": "Drafter for FE games.",
  "main": "index.js",
  "repository": "https://github.com/JazzFiend/FireEmblemDrafter.git",
  "author": "Phil Deitz <philipd@spotify.com>",
  "license": "MIT",
  "engines": {
    "node": ">=0.10.3 <0.12",
    "yarn": "^1.22.5"
  },
  "scripts": {
    "build": "webpack --mode development",
    "start": "webpack serve --mode development",
    "start:cli": "./node_modules/.bin/babel-node --presets @babel/env ./bin/draft",
    "test": "jest --testPathIgnorePatterns ./src/__endToEndTests__/",
    "test:coverage": "jest --collectCoverage --testPathIgnorePatterns ./src/__endToEndTests__/ ./src/__integrationTests__/",
    "test:e2e": "jest ./src/__endToEndTests__/",
    "test:mutation": "./node_modules/@stryker-mutator/core/bin/stryker.js run",
    "test:ui": "start-server-and-test start http://localhost:3000/ test:e2e",
    "lint": "./node_modules/.bin/eslint ./src --ext ts,tsx,js,jsx",
    "lint:fix": "./node_modules/.bin/eslint ./src --ext ts,tsx,js,jsx --fix"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "lodash": "^4.17.19",
    "prop-types": "^15.7.2",
    "puppeteer": "^13.7.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.3.0",
    "react-select": "^4.2.1",
    "readline-sync": "^1.4.9",
    "redux-mock-store": "^1.5.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@stryker-mutator/core": "^6.0.1",
